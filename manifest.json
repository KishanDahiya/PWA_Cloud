{
    "short_name": "PWA Example",
    "name": "Progressive Web Application Example",
    "start_url": "/index.html",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#000000",
    "icons": [
        {
            "src": "img/Nimg2.png",
            "sizes": "180x180",
            "type": "image/png"
        },
        {
            "src": "img/Nimg1.png",
            "sizes": "240x240",
            "type": "image/png"
        },
        {
            "src": "img/Nimg3.png",
            "sizes": "360x360",
            "type": "image/png"
        },
        {
            "src": "img/Nimg7.png",
            "sizes": "480x480",
            "type": "image/png"
        },
        {
            "src": "img/Nimg4.png",
            "sizes": "720x720",
            "type": "image/png"
        },
        {
            "src": "img/Nimg5.png",
            "sizes": "960x960",
            "type": "image/png"
        },
        {
            "src": "img/Nimg6.png",
            "sizes": "2560x2560",
            "type": "image/png"
        }
    ],
    "splash_pages": null,
    "orientation": "portrait",
    "permissions": [
        "geolocation",
        "notifications"
    ],
    "related_applications": [
        {
            "platform": "web",
            "url": "https://pwa.example.com"
        },
        {
            "platform": "play",
            "id": "com.example.app",
            "url": "https://play.google.com/store/apps/details?id=com.example.app"
        }
    ],
    "scope": "/",
    "serviceworker": {
        "src": "/sw.js",
        "scope": "/"
    },
    "prefer_related_applications": false
}